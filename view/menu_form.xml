<form string="Menu">
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="active"/>
    <field name="active"/>
    <notebook colspan="6">
        <page string="Menu" id="menu">
            <label name="website"/>
            <field name="website"/>
            <label name="target_type"/>
            <field name="target_type"/>
            <newline/>
            <label name="target_uri"/>
            <field name="target_uri"/>
            <label name="name_uri"/>
            <field name="name_uri"/>
            <label name="target_url"/>
            <field name="target_url"/>
            <newline/>
            <label name="parent"/>
            <field name="parent"/>
            <label name="sequence"/>
            <field name="sequence"/>
        </page>
        <page string="Options" id="options">
            <label name="icon"/>
            <field name="icon"/>
            <label name="css"/>
            <field name="css"/>
            <label name="login"/>
            <field name="login"/>
            <label name="manager"/>
            <field name="manager"/>
            <label name="nofollow"/>
            <field name="nofollow"/>
        </page>
        <page string="Responsive" id="responsive">
            <group col="10" colspan="6" id="hidden">
                <label name="hidden_xs"/>
                <field name="hidden_xs"/>
                <label name="hidden_sm"/>
                <field name="hidden_sm"/>
                <label name="hidden_md"/>
                <field name="hidden_md"/>
                <label name="hidden_lg"/>
                <field name="hidden_lg"/>
            </group>
        </page>
    </notebook>
</form>
