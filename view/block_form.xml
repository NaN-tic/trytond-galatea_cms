<form string="Block">
    <label name="name"/>
    <field name="name"/>
    <label name="code"/>
    <field name="code"/>
    <label name="type"/>
    <field name="type"/>
    <label name="click_url"/>
    <field name="click_url"/>
    <label name="active"/>
    <field name="active"/>
    <notebook colspan="4">
        <page string="CMS" id="cms">
            <label name="file"/>
            <field name="file"/>
            <label name="remote_image_url"/>
            <field name="remote_image_url"/>
            <label name="height"/>
            <field name="height"/>
            <label name="width"/>
            <field name="width"/>
            <separator name="custom_code" colspan="4"/>
            <field name="custom_code" colspan="4"/>
            <label name="title"/>
            <field name="title"/>
            <label name="show_title"/>
            <field name="show_title"/>
            <label name="title_headings"/>
            <field name="title_headings"/>
            <label name="markup"/>
            <field name="markup"/>
            <separator name="paragraph1" colspan="4"/>
            <field name="paragraph1" colspan="4"/>
            <separator name="paragraph2" colspan="4"/>
            <field name="paragraph2" colspan="4"/>
            <separator name="paragraph3" colspan="4"/>
            <field name="paragraph3" colspan="4"/>
            <separator name="paragraph4" colspan="4"/>
            <field name="paragraph4" colspan="4"/>
            <separator name="paragraph5" colspan="4"/>
            <field name="paragraph5" colspan="4"/>
            <separator string="Cover Images" colspan="4"/>
            <label name="cover_image_align"/>
            <field name="cover_image_align"/>
            <label name="cover_image1"/>
            <field name="cover_image1"/>
            <label name="cover_image2"/>
            <field name="cover_image2"/>
            <label name="cover_image3"/>
            <field name="cover_image3"/>
            <label name="cover_image4"/>
            <field name="cover_image4"/>
            <label name="cover_image5"/>
            <field name="cover_image5"/>
        </page>
        <page string="Extra" id="extra">
            <label name="visibility"/>
            <field name="visibility"/>
            <label name="css"/>
            <field name="css"/>
        </page>
        <page string="SEO" id="seo">
            <label name="alternative_text"/>
            <field name="alternative_text"/>
        </page>
        <page string="Attachments" id="attachments">
            <field name="attachments"/>
        </page>
    </notebook>
</form>
